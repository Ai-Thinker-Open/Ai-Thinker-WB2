        case WIFI_STATE_CONNECTED_IP_GOT:
            g_bl_ops_funcs._printf("wifi state connected ip got\r\n");
            break;
        case WIFI_STATE_DISCONNECT:
            g_bl_ops_funcs._printf("wifi state disconnect\r\n");
            break;
        case WIFI_STATE_WITH_AP_IDLE:
            g_bl_ops_funcs._printf("wifi state with ap idle\r\n");
            break;
        case WIFI_STATE_WITH_AP_CONNECTING:
            g_bl_ops_funcs._printf("wifi state with ap connecting\r\n");
            break;
        case WIFI_STATE_WITH_AP_CONNECTED_IP_GETTING:
            g_bl_ops_funcs._printf("wifi state with ap connected ip getting\r\n");
            break;
        case WIFI_STATE_WITH_AP_CONNECTED_IP_GOT:
            g_bl_ops_funcs._printf("wifi state with ap connected ip got\r\n");
            break;
        case WIFI_STATE_WITH_AP_DISCONNECT:
            g_bl_ops_funcs._printf("wifi state with ap disconnect\r\n");
            break;
        case WIFI_STATE_IFDOWN:
            g_bl_ops_funcs._printf("wifi state ifdown\r\n");
            break;
        case WIFI_STATE_SNIFFER:
            g_bl_ops_funcs._printf("wifi state sniffer\r\n");
            break;
        case WIFI_STATE_PSK_ERROR:
            g_bl_ops_funcs._printf("wifi state psk error\r\n");
            break;
        case WIFI_STATE_NO_AP_FOUND:
