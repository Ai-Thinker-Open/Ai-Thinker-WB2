    do { if( ( ret = (mbedtls_mpi_mul_mpi(&tmp, &tmp, a)) ) != 0 ) goto cleanup; } while( 0 );
    do { if( ( ret = (mpi_mod_2n(&tmp, m)) ) != 0 ) goto cleanup; } while( 0 );
    do { if( ( ret = (mbedtls_mpi_shift_l(U, 1)) ) != 0 ) goto cleanup; } while( 0 );
    do { if( ( ret = (mbedtls_mpi_sub_mpi(U, U, &tmp)) ) != 0 ) goto cleanup; } while( 0 );
